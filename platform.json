    "packages": {
        "toolchain-gccarmnoneeabi": {
            "type": "toolchain",
            "version": ">=1.40803.0,<1.40805.0"
        },
        "framework-stm32duino": {
            "type": "framework",
            "version": "~1.10609.0"
        },
        "tool-openocd": {
            "type": "uploader",
            "optional": true,
            "version": "~1.900.0"
        }
    }

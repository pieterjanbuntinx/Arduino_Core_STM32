{
    "name": "stm32duino",
    "title": "STM32duino",
    "description": "Arduino STM32",
    "url": "http://www.stm32duino.com",
    "homepage": "http://www.stm32duino.com",
    "license": {
        "type": "Apache-2.0",
        "url": "http://opensource.org/licenses/apache2.0.php"
    },
    "engines": {
        "platformio": "~3.0.0",
        "scons": ">=2.3.0,<2.6.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pieterjanbuntinx/Arduino_Core_STM32"
    },
    "version": "0.0.2",
    "packageRepositories": [
        "https://github.com/pieterjanbuntinx/Arduino_Core_STM32",
        "https://dl.bintray.com/platformio/dl-packages/manifest.json",
        "https://sourceforge.net/projects/platformio-storage/files/packages/manifest.json/download",
        "http://dl.platformio.org/packages/manifest.json"
    ],
    "frameworks": {
        "arduino": {
            "package": "framework-stm32duino",
